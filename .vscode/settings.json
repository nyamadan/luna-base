{
  "cmake.generator": "Ninja",
  "cmake.debugConfig": {
    "cwd": "${workspaceFolder}",
    "args": ["./scripts/main/dist/index.lua"]
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "files.associations": {
    "algorithm": "cpp",
    "charconv": "cpp",
    "fstream": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "xiosbase": "cpp",
    "xstring": "cpp",
    "cstdint": "cpp",
    "*.rh": "c"
  },
  "cSpell.words": [],
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools"
}
