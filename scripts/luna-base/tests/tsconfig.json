{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/vscode-typescript-to-lua/master/tsconfig-schema.json",
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "declaration": false
  },
  "tstl": {
    "luaLibImport": "require",
    "buildMode": "default",
    "luaBundle": "index.lua",
    "luaBundleEntry": "index.ts",
    "luaTarget": "5.4",
    "noResolvePaths": ["math"]
  },
  "exclude": [],
  "include": ["./index.ts"]
}
